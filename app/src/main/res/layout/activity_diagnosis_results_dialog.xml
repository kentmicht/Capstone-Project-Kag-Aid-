<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/find_nearby_doctors2"
        android:layout_width="66dp"
        android:layout_height="65dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/common_treatments2"
        android:layout_marginStart="87dp"
        android:background="@drawable/nearby_doctors"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="185dp" />

    <Button
        android:id="@+id/common_treatments2"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="269dp"
        android:background="@drawable/skin_illness_and_treatments_logo"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="185dp" />

    <ImageView
        android:id="@+id/capturedPictureView"
        android:layout_width="133dp"
        android:layout_height="132dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/ic_center_focus_strong_black_24dp"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="6dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="174dp"
        android:text="Accuracy"
        android:textColor="@color/colorPrimary"
        android:textSize="14sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="138dp" />

    <TextView
        android:id="@+id/skin_illness_accuracy"
        android:layout_width="51dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:text="80%"
        android:textAlignment="center"
        android:textSize="14sp"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="275dp"
        tools:layout_editor_absoluteY="138dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="219dp"
        android:text="Last Scanned"
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="166dp" />

    <TextView
        android:id="@+id/datetime_scanned"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView14"
        android:layout_centerHorizontal="true"
        android:text="	Aug 29, 2018	2:30:39 PM"
        android:textStyle="italic"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="166dp" />

    <Button
        android:id="@+id/ok_button"
        android:layout_width="61dp"
        android:layout_height="63dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/find_nearby_doctors2"
        android:layout_marginEnd="88dp"
        android:background="@drawable/ok_logo"
        android:textColor="@color/textHomepage"
        tools:layout_editor_absoluteX="237dp"
        tools:layout_editor_absoluteY="194dp" />

    <TextView
        android:id="@+id/skin_illness_name"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/capturedPictureView"
        android:layout_centerHorizontal="true"
        android:text="Acne"
        android:textAlignment="center"
        android:textColor="@color/colorPrimary"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="138dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/find_nearby_doctors2"
        android:layout_centerHorizontal="true" />
</RelativeLayout>