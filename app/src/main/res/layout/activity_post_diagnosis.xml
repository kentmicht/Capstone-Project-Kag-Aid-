<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/postDiagnosis"
    android:layout_height="match_parent"
    tools:context=".Diagnosis.PostDiagnosis">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="match_parent"
        android:layout_height="107dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@color/textHomepage" />

    <TextView
        android:id="@+id/skinIllness"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="144dp"
        android:text="Acne Open Commedo"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="167dp"
        tools:layout_editor_absoluteY="97dp" />

    <TextView
        android:id="@+id/percentage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/skinIllness"
        android:layout_centerHorizontal="true"
        android:paddingRight="5sp"
        android:text="Percentage: 100%"
        android:textAlignment="center"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="122dp" />

    <TextView
        android:id="@+id/question1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/question3"
        android:layout_marginTop="212dp"
        android:text="When did the skin illness occur?"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/question2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ans2"
        android:layout_alignStart="@+id/question1"
        android:text="Is the skin illness itchy?"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="254dp" />

    <TextView
        android:id="@+id/question3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="260dp"
        android:layout_marginStart="49dp"
        android:text="What were the over-the-counter treatments used?"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="339dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="137dp"
        android:text="Button"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="436dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/question3"
        android:layout_marginEnd="37dp"
        android:layout_marginTop="-260dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="191dp" />

    <EditText
        android:id="@+id/ans2"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/question1"
        android:layout_marginBottom="295dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="273dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/question1"
        android:layout_below="@+id/question1"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="358dp" />
</RelativeLayout>